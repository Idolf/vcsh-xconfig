! Fix Caps lock
keycode 66  = Escape NoSymbol Escape
keycode 78  = Caps_Lock
clear lock
add   lock  = Caps_Lock

! Remap Windows -> Alt_L and Alt_L -> Ctrl_R
keycode 64  = Super_L NoSymbol Super_L
keycode 133 = Alt_L Meta_L Alt_L Meta_L
clear mod1
clear mod4
add   mod1 = Alt_L Meta_L
add   mod4 = Super_L
